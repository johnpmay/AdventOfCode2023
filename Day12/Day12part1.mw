<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">findMatches := proc(rr, nn, match_so_far::nonnegint:=0) #option trace;
local i, dstr, ostr, chk, p, tot:=0;
option cache;
     p := parity(rr);
     if p &gt; add(nn) then return 0; end if;

     i := FirstFromLeft(&quot;?&quot;, rr);
     if i = 0 then
         chk := CheckData2(rr, nn);
         if chk = true then
          # print(rr);          
            return 1;
         elif type(chk, integer) and chk &lt; nops(nn) then
           #error &quot;snh 1&quot;;
            return 0;
         else 
           # error &quot;snh 2&quot;;
            return 0;
         end if;
      end if;


      if p &lt; add(nn) then
          dstr := cat(rr[1..i-1],&quot;#&quot;,rr[i+1..-1]);
         chk := CheckData(dstr, nn, i);
         if op(0,chk) &lt;&gt; 'fail' and chk &gt;= match_so_far then
              tot := tot+thisproc(dstr, nn, chk);
         end if;
      else
         return thisproc( Subs(&quot;?&quot;=&quot;.&quot;,rr), nn, match_so_far );
      end if;
      ostr := cat(rr[1..i-1],&quot;.&quot;,rr[i+1..-1]);
      chk := CheckData(ostr, nn, i);
      if op(0,chk) &lt;&gt; 'fail' and chk &gt;= match_so_far then
          tot := tot+thisproc(ostr, nn, chk);
      end if;

      return tot;

end proc:

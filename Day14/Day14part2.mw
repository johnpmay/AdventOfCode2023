radix := max(map(length, subs(&quot;&quot;=NULL, Split( cat(map(l-&gt;(l[],&quot;#&quot;), convert( grid, listlist))[]), &quot;#&quot;))));fingerprint := proc() local i, d := map(s-&gt;length(Subs(&quot;.&quot;=&quot;&quot;,s)), subs(&quot;&quot;=NULL, Split( cat(map(l-&gt;(l[],&quot;#&quot;), convert( grid, listlist))[]), &quot;#&quot;)));add(d[i]*radix^(nops(d)-i),i=1..nops(d));end proc:dotilt := proc() tilt(&quot;N&quot;):tilt(&quot;W&quot;):tilt(&quot;S&quot;):tilt(&quot;E&quot;): end proc:findloop := proc(iter:=1000)local i, fp;local fptable := table(sparse=0);for i to iter do     dotilt();
fp := fingerprint();if fptable[fp] &lt;&gt; 0 then    return fptable[fp], i;else   fptable[fp] := i;end if;end do:return FAIL, FAIL;end proc:</Text-field>

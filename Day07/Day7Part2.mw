<Group hide-input="false" labelreference="L16" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">classhand := proc(g)local h := g[1];local c := ListTools:-Collect(h);if nops(c) = 5 then return 1; end if; # high cardif nops(c) = 4 then return 2; end if; # 1 pairlocal cc := sort( map2(op,2,c) );if cc = [1,2,2] then return 3; end if; # 2 pairif cc = [1,1,3] then return 4; end if; # 3 kindif cc = [2,3] then return 5; end if; # full houseif cc = [1,4] then return 6; end if; # 4 kindif cc = [5] then return 7; end if; # 5 kinderror;  end proc:</Text-field>
</Input>
</Group>

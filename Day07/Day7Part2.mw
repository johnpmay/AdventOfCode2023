<Group hide-input="false" labelreference="L25" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">classhandJ := proc(g)
local h := g[1];
local c := ListTools:-Collect(h);
local j;(j,c) := selectremove(l-&gt;l[1]=11, c);if j = [] then j := 0; else j := j[1][2]; end if;if nops(c) = 5 then return 1; end if; # high card - no j
if nops(c) = 4 then return 2; end if; # 1 pair with j or no
local cc := sort( map2(op,2,c) );
if cc = [1,1] then return 6; end if; # 3*j = 4 kindif cc = [1,1,1] then return 4; end if; # nothing + 2*j = 3 kindif cc = [1,1,2] then return 4; end if; # pair + j = 3 kindif cc = [1,2,2] then return 3; end if; # 2 pair no j
if cc = [2,2] then return 5; end if; # 2 pair + j = FHif cc = [1,2] then return 6; end if; # 2 pair + 2*j = 4 kindif cc = [1,1,3] then return 4; end if; # 3 kind
if cc = [1,3] then return 6; end if; # 3 kind + j = 4 kindif cc = [2,3] then return 5; end if; # full house
if cc = [1,4] then return 6; end if; # 4 kind
if nops(cc) &lt;= 1 then return 7; end if; # 5 kind
error &quot;unhandled hand type&quot;, j, c, cc;  
end proc:</Text-field>
</Input>
</Group>
